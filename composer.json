    "description": "Strips the JMS Serializer response parser component from misd/guzzle-bundle",
    "keywords": ["guzzle", "bundle", "http", "http client", "rest", "client", "web service", "curl", "api"],
    "homepage": "https://github.com/gbslogistics/guzzle-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Wilkinson",
            "email": "chris.wilkinson@admin.cam.ac.uk"
        },
        {
            "name": "James Haskell",
            "email": "querns@gbs.io"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "guzzle/guzzle": "~3.0",
        "jms/serializer": "~0.12.0"
    },
    "conflict": {
        "jms/serializer-bundle": "<0.11-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
